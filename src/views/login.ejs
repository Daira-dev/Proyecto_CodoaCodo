    <meta name="keywords" content="html, css, js, javascript, codo a codo">

    <!-- Título-->
    <title> Ingresar │ Funkoshop </title>
    
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous"/>
     
    <!-- CSS -->
    <link rel="stylesheet" href="../Css/index.css">
    <link rel="stylesheet" href="../Css/login.css">
</head>

<body>
  <!-- Encabezado -->
  <%- include ('partials/header') %>
  <!---->
  
  <!-- Contenido -->
  <main class="container my-5">
    <h1 class="text-center titulo">INGRESAR A MI CUENTA</h1>
    <div class="formulario d-flex justify-content-center">
      <form action="/form" method="post">
        
        <label for="text">Email:</label>
        <input id="email" name="text" type="text" placeholder="maxpower@correo.com">
        <br>
        <label for="text">Contraseña:</label>
        <input id="contraseña" name="contraseña" type="password" placeholder="•••••••••">
      </form>
    </div>
    <div class="text-center">
      <button type="submit" class="btn_1">Ingresar</button>
      <label for="recordarme" class="recordarme">
        <input type="checkbox" name="recordarme" value="si">
        Recordarme
      </label>
    </div>
    <p class="sub_text my-5 text-center"><a class="terminos_condiciones" href="./pages/login.html">Olvidé mi contraseña</a></p>

  </main>
  <!---->

  <!-- Pie de página -->
  <%- include ('partials/footerAdmin') %>  
  <!-- Fin de pie de página -->
</body>

<!-- Scripts -->
<script src="https://cdn.jsdelivr.net/npm/iconify-icon@1.0.8/dist/iconify-icon.min.js"></script>
