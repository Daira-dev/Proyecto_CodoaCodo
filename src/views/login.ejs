    <!-- Contenido -->
    <main class="container my-5">
      <h1 class="text-center titulo">INGRESAR A MI CUENTA</h1>
      <div class="formulario d-flex justify-content-center">
        <form>
          <label>Email:</label>
          <input type="text" placeholder="johndoe@correo.com">
          <br>
          <label>Contraseña:</label>
          <input type="password" placeholder="•••••••••">
        </form>
      </div>
      <div class="text-center">
        <button type="submit" class="btn_1">Ingresar</button>
        <label class="recordarme">
          <input type="checkbox" class="mycheck" name="recordarme" value="si">
          Recordarme
        </label>
        <p class="sub_text"><a href="./pages/login.html">Olvidé mi contraseña</a></p>
      </div>
    </main>
    <!---->

    <!-- Pie de página -->
    <%- include ('partials/footer') %>
    <!-- Fin de pie de página -->

  </body>

